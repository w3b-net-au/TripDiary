<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yupog2003.tripdiary"
    android:installLocation="auto"
    android:versionCode="195"
    android:versionName="1.3.7" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <permission
        android:name="com.yupog2003.tripdiary.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.yupog2003.tripdiary.permission.MAPS_RECEIVE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <!-- Permissions required for GCM -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" /> <!-- Optional permission for App measurement to run. -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name="com.yupog2003.tripdiary.TripDiaryApplication"
        android:allowBackup="true"
        android:backupAgent="com.yupog2003.tripdiary.data.MyBackupAgent"
        android:fullBackupContent="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.yupog2003.tripdiary.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yupog2003.tripdiary.ViewActivity"
            android:label="@string/title_activity_view"
            android:screenOrientation="portrait" />

        <service
            android:name="com.yupog2003.tripdiary.services.RecordService"
            android:exported="false"
            android:stopWithTask="false" />

        <activity
            android:name="com.yupog2003.tripdiary.ViewPointActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_view_point" />
        <activity
            android:name="com.yupog2003.tripdiary.PlayPointActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_play_point" />
        <activity
            android:name="com.yupog2003.tripdiary.PreferActivity"
            android:label="@string/title_activity_preference" />
        <activity
            android:name="com.yupog2003.tripdiary.CategoryActivity"
            android:label="@string/title_activity_category" />
        <activity
            android:name="com.yupog2003.tripdiary.PaintActivity"
            android:label="@string/title_activity_paint" />
        <activity android:name="com.yupog2003.tripdiary.ViewCostActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.backup.api_key"
            android:value="AEdPqrEAAAAI70FoJq0n2rVYt_ETDwHhXfMLcPdoi9lLBBtpqA" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAzPVZdjSt1ODGT9gNuTvbjZWUgpgKG-Q4" />
        <!-- publish key=  AIzaSyAzPVZdjSt1ODGT9gNuTvbjZWUgpgKG-Q4 -->
        <!-- debug key=    AIzaSyC4u43XXHl2AEk92DS-W13IC8vNDQRBJNk -->

        <service
            android:name="com.yupog2003.tripdiary.services.SendTripService"
            android:exported="false" />
        <service
            android:name="com.yupog2003.tripdiary.services.DownloadTripService"
            android:exported="false" />

        <activity
            android:name="com.yupog2003.tripdiary.ViewTripActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:documentLaunchMode="always"
            android:label="@string/view_trip"
            android:launchMode="standard"
            android:maxRecents="3"
            android:persistableMode="persistNever" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="yupog2003.idv.tw"
                    android:pathPrefix="/TripDiary/Trip.html"
                    android:scheme="http" />
                <data
                    android:host="219.85.61.62"
                    android:pathPrefix="/TripDiary/Trip.html"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yupog2003.tripdiary.AllRecordActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/lifetime_record" />
        <activity android:name="com.yupog2003.tripdiary.MyActivity" />
        <activity
            android:name="com.yupog2003.tripdiary.MultiFileChooseActivity"
            android:label="@string/import_files" />
        <activity
            android:name="com.yupog2003.tripdiary.RecordActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <service
            android:name="com.yupog2003.tripdiary.services.GenerateVideoService"
            android:exported="false" />

        <activity
            android:name="com.yupog2003.tripdiary.GetContentActivity"
            android:exported="true"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <action android:name="android.intent.action.SEND_MULTIPLE" />
                <action android:name="android.intent.action.GET_CONTENT" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.OPENABLE" />

                <data android:mimeType="image/*" />
                <data android:mimeType="video/*" />
                <data android:mimeType="audio/*" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yupog2003.tripdiary.ViewGraphActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.yupog2003.tripdiary.RestoreTripActivity"
            android:label="@string/restore_trip"
            android:theme="@style/Theme.AppCompat.Light.Translucent" >
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <action android:name="android.intent.action.SEND_MULTIPLE" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="application/zip" />
            </intent-filter>
        </activity>

        <service
            android:name="com.yupog2003.tripdiary.services.BackupRestoreTripService"
            android:exported="false" />

        <activity android:name="com.yupog2003.tripdiary.DummyActivity" />

        <service
            android:name="com.yupog2003.tripdiary.services.UploadToDriveService"
            android:exported="false" />

        <provider
            android:name="com.yupog2003.tripdiary.providers.DriveProvider"
            android:authorities="com.yupog2003.tripdiary.driveprovider"
            android:exported="true" />

        <service
            android:name="com.yupog2003.tripdiary.services.DownloadFromDriveService"
            android:exported="false" />

        <activity
            android:name="com.yupog2003.tripdiary.ImageViewerActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="false"
            android:label="@string/trip_image_viewer"
            android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <data android:mimeType="image/*" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <receiver
            android:name="com.yupog2003.tripdiary.receivers.BootCompleteReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <!-- Include the AdActivity and InAppPurchaseActivity configChanges and themes. -->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name="com.google.android.gms.ads.purchase.InAppPurchaseActivity"
            android:theme="@style/Theme.IAPTheme" />
        <activity
            android:name="com.google.android.gms.appinvite.PreviewActivity"
            android:exported="true"
            android:theme="@style/Theme.AppInvite.Preview" >
            <intent-filter>
                <action android:name="com.google.android.gms.appinvite.ACTION_PREVIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.android.gms.auth.api.signin.internal.SignInHubActivity"
            android:excludeFromRecents="true"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <!--
Service handling Google Sign-In user revocation. For apps that do not integrate with
            Google Sign-In, this service will never be started.
        -->
        <service
            android:name="com.google.android.gms.auth.api.signin.RevocationBoundService"
            android:exported="true"
            android:permission="com.google.android.gms.auth.api.signin.permission.REVOCATION_NOTIFICATION" />

        <provider
            android:name="com.google.android.gms.measurement.AppMeasurementContentProvider"
            android:authorities="com.yupog2003.tripdiary.google_measurement_service"
            android:exported="false" />

        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="com.google.android.gms.measurement.UPLOAD" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.measurement.AppMeasurementService"
            android:enabled="true"
            android:exported="false" />
    </application>

</manifest>